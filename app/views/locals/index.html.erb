<p id="notice"><%= notice %></p>

<h1>Listing Locals</h1>

<table>
  <thead>
    <tr>
      <th>Clienteid</th>
      <th>Logra</th>
      <th>Cep</th>
      <th>Uf</th>
      <th>City</th>
      <th>Km</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locals.each do |local| %>
      <tr>
        <td><%= local.cliente_id %></td>
        <td><%= local.logra %></td>
        <td><%= local.cep %></td>
        <td><%= local.uf %></td>
        <td><%= local.city %></td>
        <td><%= local.km %></td>
        <td><%= link_to 'Show', local %></td>
        <td><%= link_to 'Edit', edit_local_path(local) %></td>
        <td><%= link_to 'Destroy', local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Local', new_local_path %>
